    "dotenv": "^16.4.7",
    "formidable": "^3.5.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongodb": "^5.7.0",
    "mongoose": "^8.10.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "next-connect": "^0.12.2",
    "postcss": "^8.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "19.0.8",
