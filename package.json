    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^12.4.1",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
